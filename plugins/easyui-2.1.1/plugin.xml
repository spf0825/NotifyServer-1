<plugin name='easyui' version='2.1.1' grailsVersion='1.3.7 &gt; *'>
  <author>xiaopeng</author>
  <authorEmail>xp_mail@yahoo.com.cn</authorEmail>
  <title>provider the jquery easyui 1.2.5 for domain class Create Artifacts</title>
  <description>\
    	it is easyui  1.2.5  plugin for grails.
    	//user command "generate-all-For-EasyUi domainClassName"
       or
       user command "generate4EasyUi domainClassName"
       or
       user command "generate4EasyUi2Iframe domainClassName"
</description>
  <documentation>http://grails.org/plugin/easyui</documentation>
  <type>EasyuiGrailsPlugin</type>
  <resources>
    <resource>EasyuiController</resource>
    <resource>easyui.EasyuiTagLib</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies />
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>